{
  "name": "nuxt-helpers",
  "version": "0.8.0",
  "description": "Nuxt Helpers",
  "repository": "https://github.com/fandogh/nuxt-helpers",
  "main": "lib/install.js",
  "scripts": {
    "test": "xo",
    "fix": "xo --fix"
  },
  "contributors": [
    "Pooya Parsa <pooya@pi0.ir>"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "fs-extra": "^2.1.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "xo": "^0.18.0"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": 1,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0
    },
    "envs": [
      "browser",
      "node"
    ]
  }
}
